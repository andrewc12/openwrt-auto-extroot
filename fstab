config global
      option anon_swap '0'
      option anon_mount '0'
      option auto_swap '0'
      option auto_mount '0'
      option delay_root '3'
      option check_fs '0'

config swap
      option uuid '##SWAPUUID##'
      option enabled '1'

config mount
      option target '/overlay'
      option uuid '##ROOTUUID##'
      option fstype 'ext4'
      option options 'rw,noatime'
      option enabled '1'
      option enabled_fsck '0'

config mount
      option target '/mnt/data'
      option uuid '##DATAUUID##'
      option fstype 'ext4'
      option options 'rw,noatime'
      option enabled '1'
      option enabled_fsck '0'
